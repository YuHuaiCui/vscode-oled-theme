  "semanticHighlighting": true,
  "semanticTokenColors": {
    "templateType": {
      "foreground": "#ff0000"
    },
    "variable.globalScope": {
      "foreground": "#d0d3d5",
      "fontStyle": "bold italic"
    },
    "variable.readonly": {
      "foreground": "#F78C6C",
      "fontStyle": "bold"
    },
    "class": {
      "foreground": "#e9ba63"
    },
    "type": {
      "foreground": "#e9ba63"
    },
    "method": {
      "foreground": "#57a5e5"
    },
    "function": {
      "foreground": "#57a5e5"
    },
    "parameter": {
      "foreground": "#e16d77",
      "fontStyle": "italic bold"
    },
    "property": {
      "foreground": "#1ec4ae"
    },
    "namespace": {
      "foreground": "#e9ba63",
      "fontStyle": "italic bold"
    },
    "macro": {
      "foreground": "#1ec4ae",
      "fontStyle": "italic bold"
    }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#6a737d"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#d0d3d5"
      }
    },
    {
      "name": "Colors",
      "scope": [
        "constant.other.color"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid",
        "invalid.illegal"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "keyword.control",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#bb70d2"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.operator",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
